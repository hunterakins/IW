gm <- function(f,fi){
ret <- (2/pi**2)*( fi / (f*(f**2 - fi**2) ) )
}
gm <- function(f,fi,j){
ret <- (2/pi**2)*( fi / (f*j*(f**2 - fi**2) ) )
return (ret)
}
